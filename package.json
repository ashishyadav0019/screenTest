{
  "name": "widget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.4.1",
    "react-daterange-picker": "^2.0.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "set PORT=3232 && react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "buildCss": "node-sass-chokidar src/assets/scss/ -o src/assets/css/",
